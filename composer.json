	"description": "Youthweb OAuth 2.0 Client Provider",
	"license": "GPL3",
	"authors": [
		{
			"name": "Artur Weigandt",
			"email": "art4@wlabs.de",
			"homepage": "https://wlabs.de"
		}
	],
	"keywords": [
